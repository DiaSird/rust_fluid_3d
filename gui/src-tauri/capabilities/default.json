{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "fs:allow-write",
    "fs:allow-read",
    "fs:allow-config-read",
    "fs:allow-config-write",
    "dialog:allow-open",
    "dialog:allow-save",
    "log:default"
  ]
}
